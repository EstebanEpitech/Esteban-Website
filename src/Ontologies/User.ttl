###  http://www.semanticweb.org/User#hasName
:hasName rdf:type owl:ObjectProperty ;
         owl:inverseOf :isNameOf ;
         rdfs:domain :Person ;
         rdfs:range :Name .


###  http://www.semanticweb.org/User#hasBirthDate
:hasBirthDate rdf:type owl:ObjectProperty ;
          owl:inverseOf :isBirthDateOf ;
          rdfs:domain :date ;
          rdfs:range :BirthDate .


###  http://www.semanticweb.org/User#hasExperience
:hasExperience rdf:type owl:ObjectProperty ;
             owl:inverseOf :isExperienceOf ;
             rdfs:domain :Person ;
             rdfs:range :Experience .


###  http://www.semanticweb.org/User#hasCity
:hasCity rdf:type owl:ObjectProperty ;
            owl:inverseOf :isCityOf ;
            rdfs:domain :Place ;
            rdfs:range :City .


###  http://www.semanticweb.org/User#isCityOf
:isCityOf rdf:type owl:ObjectProperty ;
          rdfs:domain :City ;
          rdfs:range :Country .


###  http://www.semanticweb.org/User#isExperienceOf
:isExperienceOf rdf:type owl:ObjectProperty ;
