###  http://www.semanticweb.org/User#Person_of_Interest
:Person_of_Interest rdf:type owl:Class ;
    rdfs:subClassOf :Person ,
                    [ rdf:type owl:Restriction ;
                      owl:onProperty :hasSubject ;
                      owl:someValuesFrom :Subject_of_interest
                    ] ,
                    [ rdf:type owl:Restriction ;
                      owl:onProperty :hasSubject ;
                      owl:someValuesFrom :Subject_of_interest2
                    ] ,
                    [ rdf:type owl:Restriction ;
                      owl:onProperty :hasExperience ;
                      owl:allValuesFrom :Master
                    ] ,
                    [ rdf:type owl:Restriction ;
                      owl:onProperty :hasLanguage ;
                      owl:allValuesFrom :English
                    ] .


###  http://www.semanticweb.org/User#Bachelor
:Bachelor rdf:type owl:Class ;
          rdfs:subClassOf :Experience .


###  http://www.semanticweb.org/User#Country
:Country rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :hasCity ;
                           owl:someValuesFrom :City
                         ] .


###  http://www.semanticweb.org/User#Experience
:Experience rdf:type owl:Class ;
       rdfs:comment "Experience of a user" .


###  http://www.semanticweb.org/User#Master
:Master rdf:type owl:Class ;
        rdfs:subClassOf :Exeperience .


